<table class="table">
    <tr>
        <th>id</th>
        <th>status</th>
        <th>description</th>
    </tr>
    <% tasks.forEach((task) => { %>
        <tr>
            <td><%=task.id%></td>
            <td><%=task.status%></td>
            <td><%=task.description%></td>
        </tr>
    <% }) %>
</table>




